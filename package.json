{
  "author": "",
  "dependencies": {
    "@wopr-network/plugin-types": "^0.2.1",
    "hyperswarm": "^4.7.15",
    "winston": "^3.11.0",
    "zod": "^3.23.0"
  },
  "description": "P2P networking plugin for WOPR with Hyperswarm, identity management, and A2A tools",
  "devDependencies": {
    "@biomejs/biome": "^2.3.15",
    "@types/node": "^18.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0",
    "semantic-release": "^24.0.0",
    "@wopr-network/semantic-release-config": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "wopr",
    "wopr-plugin",
    "p2p",
    "hyperswarm",
    "peer-to-peer",
    "a2a"
